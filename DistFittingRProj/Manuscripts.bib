journal = {Forest Ecology and Management},
keywords = {Decision support,Fire danger,Forest restoration,Fuels management,Landscape analysis,Planning},
month = {nov},
number = {11},
pages = {2373--2381},
title = {{National fuel-treatment budgeting in US federal agencies: Capturing opportunities for transparent decision-making}},
url = {http://www.sciencedirect.com/science/article/pii/S037811270900574X http://linkinghub.elsevier.com/retrieve/pii/S037811270900574X},
volume = {258},
