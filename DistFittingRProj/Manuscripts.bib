author = {Bechtold, William A and Patterson, Paul L},
booktitle = {USDA General Technical Report},
file = {:D$\backslash$:/MendeleyDocs/Bechtold, Patterson - 2005 - The Enhanced Forest Inventory and Analysis Program — National Sampling Design and Estimation Procedures.pdf:pdf},
institution = {Southern Research Station},
keywords = {Annual inventory,FIA,forest,forest health monitoring,inventory,plot design,sampling frame.},
pages = {85},
title = {{The Enhanced Forest Inventory and Analysis Program — National Sampling Design and Estimation Procedures}},
url = {https://www.srs.fs.usda.gov/pubs/gtr/gtr{\%}7B{\_}{\%}7Dsrs080/gtr{\%}7B{\_}{\%}7Dsrs080.pdf},
volume = {SRS-80},
year = {2005}
}
