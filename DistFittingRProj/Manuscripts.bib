@article{Riccardi2007,
abstract = {Wildland fuel characteristics are used in many applications of operational fire predictions and to understand fire effects and behaviour. Even so, there is a shortage of information on basic fuel properties and the physical characteristics of wildland fuels. The Fuel Characteristic Classification System (FCCS) builds and catalogues fuelbed descriptions based on realistic physical properties derived from direct or indirect observation, inventories, expert knowledge, inference, or simulated fuel characteristics. The FCCS summarizes and calculates wildland fuel characteristics, including fuel depth, loading, and surface area. Users may modify fuelbeds and thereby capture changing fuel conditions over time and (or) under different management prescriptions. Fuel loadings from four sample fuelbed pairs (i.e., pre- and post-prescribed fire) were calculated and compared by using FCCS to demonstrate the versatility of the system and how individual fuel components, such as shrubs, nonwoody fuels, woody fuels, and litter, can be calculated and summarized. The ability of FCCS to catalogue and summarize complex fuelbeds and reflect dynamic fuel conditions allows calculated results to be used in a variety of applications including surface and crown fire predictions, carbon assessments, and wildlife habitat management.},
annote = {This is another article on the Fuel Characteristic Classification System (FCCS). It gives an overall description of the FCCS. The authors claim that the ability to catalogue and summarize the complex fuel conditions can be used in a variety of applications. We can use this in our study to describe the FCCS.},
author = {Riccardi, Cynthia L. and Prichard, Susan J. and Sandberg, David V. and Ottmar, Roger D.},
doi = {10.1139/X07-175},
file = {:D$\backslash$:/MendeleyDocs/Riccardi et al. - 2007 - Quantifying physical characteristics of wildland fuels using the Fuel Characteristic Classification SystemThis.pdf:pdf},
isbn = {0045-5067, 1208-6037},
issn = {0045-5067},
journal = {Canadian Journal of Forest Research},
month = {dec},
number = {12},
pages = {2413--2420},
title = {{Quantifying physical characteristics of wildland fuels using the Fuel Characteristic Classification SystemThis article is one of a selection of papers published in the Special Forum on the Fuel Characteristic Classification System.}},
url = {http://www.nrcresearchpress.com/doi/abs/10.1139/X07-175},
volume = {37},
year = {2007}
