@article{Brandis2003,
abstract = {Fuel loads in forest areas are dependent on vegetation type and the time since the last fire. This paper reports a study on the feasibility of using remotely sensed data to estimate vegetative fuel loads. It describes two methods for estimating fuel loads using Landsat TM data based on equations describing litter accumulation and decomposition. The first method uses classification techniques to predict vegetation types coupled with fire history data to derive current fuel loads. The second method applies a canopy turnover rate to estimate litterfall and subsequently accumulated litter from biomass, thus utilising the dominant influence of canopy on remotely sensed data. Both methods are compared with data collected from Popran National Park in coastal New South Wales. The amounts of litter calculated with the biomass method were similar to field results, but the classification method was found to overestimate fuel loads. A sensitivity analysis investigated the impact of varying the vegetation constants and rates used in the fuel estimates to simulate uncertainty or error in their values. The biomass method was less subject to uncertainties and has potential for estimating fuel quantities to provide useful spatial information for fire managers.},
annote = {This study describes how fuel loads in forest areas are dependent on the type of vegetation and the time since the last fire. Brandis and Jacobson go on in the introduction to say that vegetative fuel load information is important because fire managers need to be able to quantify the amount of fuel the fire has available to burn. This study also refers to a study about the feasibility of remote sensing to estimate fuel loads. One of Brandis and Jacobson's conclusions were that if fire history data is present it is possible to estimate fuel loads from remotely sensed data. Using Conroy's equation in predicting biomass turnover, the fuel quantities were closer to the actual quantities. They concluded that this would have potential to measure loads in remote and rugged locations. Accuracy of fuel load estimates would be improved if there were better estimates of vegetation data. Remote sensing has advantages over the current methods like; time in the field would be reduced, data for remote areas could be obtained, and imagery could provide a range of other related information.},
author = {Brandis, Kate and Jacobson, Carol},
doi = {10.1071/WF03032},
file = {:D$\backslash$:/MendeleyDocs/Brandis, Jacobson - 2003 - Estimation of vegetative fuel loads using Landsat TM imagery in New South Wales, Australia.pdf:pdf},
isbn = {1049-8001},
issn = {1049-8001},
journal = {International Journal of Wildland Fire},
keywords = {Fire history,Litterfall,Remote sensing,Wildfire},
number = {2},
pages = {185--194},
title = {{Estimation of vegetative fuel loads using Landsat TM imagery in New South Wales, Australia}},
url = {http://www.publish.csiro.au/wf/pdf/WF03032 http://www.publish.csiro.au/?paper=WF03032},
volume = {12},
year = {2003}
}
@techreport{Albini1976,
abstract = {This paper presents a brief survey of the research literature on wildfire behavior and effects and assembles formulae and graphical computation aids based on selected theoretical and empirical models. The uses of mathematical fire behavior models are discussed, and the general capabilities and limitations of currently available models are outlined. Rothermel's fire spread model is used to develop nomographs for estimating rate of spread, reaction intensity, and flame length for a variety of "typical" fuel complexes, under widely variable conditions. Factors affecting spread rate and overall shape of a fire are quantified, as well as some fire effects such as crown scorching and duff removal. Appendices give more details of the formulations presented graphically in the text, including the definitions of terms used to quantify fire behavior and effects and tables of numerical factors for converting values to different units of measurement},
address = {Ogden, Utah},
annote = {This document gives a description on estimation of fire behavior. This report introduces tools to fire specialists so that they can accurately predict fire behaviour. This study uses the process of constructive “feedback” to create useful research efforts that can better fit the needs of the people who use those research results. There are many equations that give a quantitative value to the various characteristics of fire behavior. This report can be used in this study to classify different types of fuels.},
author = {Albini, Frank A.},
file = {:D$\backslash$:/MendeleyDocs/Albini - 1976 - Estimating Wildfire Behavior and Effects.pdf:pdf},
institution = {Rocky Mountain Research Station},
pages = {1 -- 91},
