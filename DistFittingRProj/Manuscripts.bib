@article{Huang2009,
abstract = {The Coarse Woody Debris (CWD) quantity, defined as biomass per unit area (t/ha), and the quality, defined as the proportion of standing dead logs to the total CWD quantity, greatly contribute to many ecological processes such as forest nutrient cycling, tree regeneration, wildlife habitat, fire dynamics, and carbon dynamics. However, a cost-effective and time-saving method to determine CWD is not available. Very limited literature could be found that applies remote sensing technique to CWD inventory. In this paper, we fused the wall-to-wall multi-frequency and multi-polarization Airborne Synthetic Aperture Radar (AirSAR) and optical Airborne Visible/Infrared Imaging Spectrometer (AVIRIS) to estimate the quantity and quality of CWD in Yellowstone post-fire forest ecosystem, where the severe 1988 fire event resulted in high spatial heterogeneity of dead logs. To relate backscatter values to CWD metrics, we first reduced the terrain effect to remove the interference of topography on AirSAR backscatter. Secondly, we removed the influence of regenerating sapling by quadratic polynomial fitting between AVIRIS Enhanced Vegetation Index (EVI) and different channels backscatters. The quantity of CWD was derived from Phh and Phv, and the quality of CWD was derived from Phh aided by the ratio of Lhv and Phh. Two maps of Yellowstone post-fire CWD quantity and quality were produced. The calculated CWD quantity and quality were validated by extensive field surveys. Regarding CWD quantity, the correlation coefficient between measured and predicted CWD is only 0.54 with mean absolute error up to 29.1 t/ha. However, if the CWD quantity was discretely classified into three categories of "≤ 60", "60-120", and "≥ 120", the overall accuracy is 65.6{\{}{\%}{\}}; if classified into two categories of "≤ 90" and "≥ 90", the overall accuracy is 73.1{\{}{\%}{\}}; if classified into two categories of "≤ 60" and "≥ 60", the overall accuracy is 84.9{\{}{\%}{\}}. This indicates our attempt to map CWD quantity spatially and continuously achieved partial success; however, the general and discrete categories are reasonable. Regarding CWD quality, the overall accuracy of 5 types (Type 1-standing CWD ratio ≥ 40{\{}{\%}{\}}; Type 2-15{\{}{\%}{\}} ≤ standing CWD ratio {\{}{\textless}{\}} 40{\{}{\%}{\}}; Type 3-7{\{}{\%}{\}} ≤ standing CWD ratio{\{}{\textless}{\}} 15{\{}{\%}{\}}; Type 4-3{\{}{\%}{\}} ≤ standing CWD ratio {\{}{\textless}{\}} 7{\{}{\%}{\}}; Type 5-standing CWD ratio {\{}{\textless}{\}} 3{\{}{\%}{\}}) is only 40.32{\{}{\%}{\}}. However, when type 1, 2, 3 are combined into one category and type 4 and 5 are combined into one category, the overall accuracy is 67.74{\{}{\%}{\}}. This indicates the partial success of our initial results to map CWD quality into detailed categories, but the result is acceptable if solely very coarse CWD quality is considered. Bias can be attributed to the complex influence of many factors, such as field survey error, sapling compensation, terrain effect reduction, surface properties, and backscatter mechanism understanding. {\{}{\textcopyright}{\}} 2009 Elsevier Inc.},
annote = {This study explains how important Coarse Woody Debris (CWD) is to the processes of a forested area, like Yellowstone. These processes can include nutrient cycling, tree regeneration, wildlife habitat, fire dynamics and carbon dynamics. the authors combine the "wall-to-wall" multi-frequency and multi-polarization Airborne Synthetic Aperture Radar (AirSAR) and optical Airborne Visible/ Infrared Imaging Spectrometer (AVIRIS). These are used to estimate the quantity and quality of CWD's in Yellowstone's post-fire environment (after the 1988 fire event). The promise of CWD assessment is promising with improved models resulting from saplings and topography. we can use this in our study to show the uniqueness of ours. And to show a study that evaluated other fuel maps.},
author = {Huang, Shengli and Crabtree, Robert L and Potter, Christopher and Gross, Peggy},
doi = {10.1016/j.rse.2009.05.001},
file = {:D$\backslash$:/MendeleyDocs/Huang et al. - 2009 - Estimating the quantity and quality of coarse woody debris in Yellowstone post-fire forest ecosystem from fusion o.pdf:pdf},
isbn = {0034-4257},
issn = {00344257},
journal = {Remote Sensing of Environment},
keywords = {Coarse woody debris,Data fusion,Forest fire,Remote sensing,SAR,Yellowstone,coarse woody debris,data fusion,forest fire,remote sensing},
mendeley-tags = {SAR,Yellowstone,coarse woody debris,data fusion,forest fire,remote sensing},
month = {sep},
number = {9},
pages = {1926--1938},
publisher = {Elsevier Inc.},
title = {{Estimating the quantity and quality of coarse woody debris in Yellowstone post-fire forest ecosystem from fusion of SAR and optical data}},
url = {http://dx.doi.org/10.1016/j.rse.2009.05.001 http://linkinghub.elsevier.com/retrieve/pii/S0034425709001497 http://www.sciencedirect.com/science/article/pii/S0034425709001497},
volume = {113},
year = {2009}
}
@incollection{Brown1974,
abstract = {To facilitate debris management, procedures for inventorying downed woody material are presented. Instructions show how to estimate weights and volumes of downed woody material, fuel depth, and duff depth. Using the planar intersect technique, downed material is inventoried by 0- to 0.25-inch, 0.25- to 1-inch, and 1- to 3-inch diameter classes; and by 1-inch classes for sound and rotten pieces over 3 inches. The method is rapid and easy to use and can be applied to naturally fallen debris and to slash. The method involves counting downed woody pieces that intersect vertical sampling planes and measuring the diameters of pieces larger than 3 inches in diameter. The piece counts and diameters permit calculation of tons per acre.},
address = {Cambridge},
annote = {This is a handbook gives directions on how to identify weights, volumes, and depths of downed woody material. In this report downed woody material is described as the dead twigs, branches, stems, and boles of trees and brush that have fallen and now lie on the ground. This material is called slash or logging debris in this report if it was accumulated by human activity and is called fuel, natural debris, or detritus if the accumulation is natural. Inventorying these downed woody material can help fire and land managers estimate utilization potentials. This report can be used in this study to describe measurement protocols for duff and litter depths.},
archivePrefix = {arXiv},
arxivId = {arXiv:1011.1669v3},
author = {Brown, James K},
booktitle = {Climate Change 2013 - The Physical Science Basis},
doi = {10.1017/CBO9781107415324.004},
editor = {{Intergovernmental Panel on Climate Change}},
eprint = {arXiv:1011.1669v3},
file = {:D$\backslash$:/MendeleyDocs/Brown - 1974 - Summary for Policymakers.pdf:pdf},
isbn = {INT-16},
issn = {1098-6596},
keywords = {debris,fire causes (forest),forest fuels,fuel inventory,planar intersect method,sampling methods},
pages = {1--30},
pmid = {155},
publisher = {Cambridge University Press},
title = {{Summary for Policymakers}},
url = {http://ebooks.cambridge.org/ref/id/CBO9781107415324A009},
year = {1974}
}
@article{Domke2013,
abstract = {The inventory and monitoring of coarse woody debris (CWD) carbon (C) stocks is an essential component of any comprehensive National Greenhouse Gas Inventory (NGHGI). Due to the expense and difficulty associated with conducting field inventories of CWD pools, CWD C stocks are often modeled as a function of more commonly measured stand attributes such as live tree C density. In order to assess potential benefits of adopting a field-based inventory of CWD C stocks in lieu of the current model-based approach, a national inventory of downed dead wood C across the U.S. was compared to estimates calculated from models associated with the U.S.'s NGHGI and used in the USDA Forest Service, Forest Inventory and Analysis program. The model-based population estimate of C stocks for CWD (i.e., pieces and slash piles) in the conterminous U.S. was 9 percent (145.1 Tg) greater than the field-based estimate. The relatively small absolute difference was driven by contrasting results for each CWD component. The model-based population estimate of C stocks from CWD pieces was 17 percent (230.3 Tg) greater than the field-based estimate, while the model-based estimate of C stocks from CWD slash piles was 27 percent (85.2 Tg) smaller than the field-based estimate. In general, models overestimated the C density per-unit-area from slash piles early in stand development and underestimated the C density from CWD pieces in young stands. This resulted in significant differences in CWD C stocks by region and ownership. The disparity in estimates across spatial scales illustrates the complexity in estimating CWD C in a NGHGI. Based on the results of this study, it is suggested that the U.S. adopt field-based estimates of CWD C stocks as a component of its NGHGI to both reduce the uncertainty within the inventory and improve the sensitivity to potential management and climate change events.},
annote = {Inventorying coarse woody debris (CWD) and carbon stocks are a vital element of the National Greenhouse GAs Inventory (NGHGI). These attributes are hard to physically measure so stocks are modeled as a function of more commonly known or, measured attributes. This study concluded that coarse woody debris carbon density is not entirely a function of the forest stands carbon stocks. Many factors complicate the relationship between live and dead carbon wood stocks. The models in this study may provide plausible coarse woody debris carbon population at the national scale but does introduce some bias into the model. The models will always predict coarse woody debris carbon stocks wherever the live tree carbon stocks are well understood. However, the models greatly underestimate the coarse woody debris carbon stocks when morality events result in low live carbon stocks. We can use this to show that there is some uncertainty in the modeling of fuels.},
author = {Domke, Grant M. and Woodall, Christopher W. and Walters, Brian F. and Smith, James E.},
doi = {10.1371/journal.pone.0059949},
editor = {Bond-Lamberty, Ben},
file = {:D$\backslash$:/MendeleyDocs/Domke et al. - 2013 - From Models to Measurements Comparing Downed Dead Wood Carbon Stock Estimates in the U.S. Forest Inventory.pdf:pdf},
isbn = {19326203},
issn = {1932-6203},
journal = {PLoS ONE},
month = {mar},
number = {3},
pages = {e59949},
pmid = {23544112},
title = {{From Models to Measurements: Comparing Downed Dead Wood Carbon Stock Estimates in the U.S. Forest Inventory}},
url = {http://journals.plos.org/plosone/article/file?id=10.1371/journal.pone.0059949{\&}type=printable http://dx.plos.org/10.1371/journal.pone.0059949},
volume = {8},
year = {2013}
}
@techreport{Keane1998,
abstract = {Fuel and vegetation spatial data layers required by the spatially explicit fire growth model FARSITE were developed for all lands in and around the Selway-Bitterroot Wilderness Area in Idaho and Montana. Satellite imagery and terrain modeling were used to create the three base vegetation spatial data layers of potential vegetation, cover type, and structural stage. Fire behavior fuel models and crown characteristics were assigned to combinations of base layer categories on these maps by local fire managers, ecologists, and existing data. FARSITE fuels maps are used to simulate growth of prescribed natural fires in the wilderness area, aiding managers in the planning and allocation of resources. An extensive accuracy assessment of all maps indicated fuels layers are about 60 percent accurate. This methodology was designed to be replicated for other areas.},
address = {Fort Collins, CO},
annote = {FARSITE is a spatially explicit fire growth model. This is used to predict the size, intensity, spread, and rate of wildland fires. This model requires eight data layers: fire behavior fuel model, crown closure and height, stand height, crown bulk density, elevation, aspect, and slope. These were created from a digital terrain model. Potential vegetation types were modeled from topographic data layers based on modeling workshops. Cover type and stage layers were acquired form satellite imagery. Fuel models were assigned to each potential vegetation type cover type and structural stage. An accuracy assessment was performed on the layers. The potential vegetation type layer is 60 percent accurate. We can use this in our study to show the accuracy of fuel models.},
author = {Keane, Robert E. and Garner, Janice L and Schmidt, Kirsten M and Long, Donald G and Menakis, James P and Finney, Mark A.},
booktitle = {General Technical Report RMRS-GTR-3},
file = {:D$\backslash$:/MendeleyDocs/Keane et al. - 1998 - Development of input data layers for the FARSITE fire growth model for the Selway-Bitterroot wilderness complex, U.pdf:pdf},
institution = {United States Department of Agriculture},
isbn = {4063294846},
issn = {02775786},
keywords = {GIS,fire behavior fuel model,fuels mapping,satellite imagery,terrain modeling},
pages = {66},
title = {{Development of input data layers for the FARSITE fire growth model for the Selway-Bitterroot wilderness complex, USA}},
url = {https://www.fs.fed.us/rm/pubs/rmrs{\_}gtr003.pdf},
year = {1998}
}
@incollection{Harmon2004,
abstract = {1. Coarse woody debris (CWD) is an important component of temperate stream and forest ecosystems. We have reviewed the rates at which CWD is added and removed from ecosystems, the biomass found in streams and forests, and many functions that CWD serves. 2. CWD is added to ecosystems by numerous mechanisms, including wind, fire, insect attack, pathogens, competition, and geomorphic processes. Despite the many long-term studies on tree mortality, there are few published rates of CWD input on mass-area-1 time-1 basis. Most ecological studies have not measured CWD input over a long enough period or a large enough area to give accurate estimates. Input rates measured in temperate ecosystems range from 0.12 to 14.9 Mg ha-1 year-1 and vary greatly over time and space. 3. Once CWD enters the detrital food web, it is decomposed by a large array of organisms and physical processes. Although respiration-caused losses have been the focus of many studies, CWD is also significantly transformed physically and chemically. Movement of CWD, especially in streams, is also an important but poorly documented mechanism whereby CWD is lost from ecosystems. Many factors control the rate at which CWD decomposes, including temperature, moisture, the internal gas composition of CWD, substrate quality, the size of the CWD, and the types of organisms involved. However, the importance of many of these factors has yet to be established in field experiments. 4. The mass of CWD in an ecosystem ideally represents the balance between addition and loss. In reality, slow decomposition rates and erratic variations in input of CWD cause the CWD mass to deviate markedly from steady-state projections. The mass of CWD in stream and forest ecosystems varies widely, ranging between 1 and 269 Mg ha-1. Many differences correspond to forest type, with deciduous-dominated systems having generally lower biomass than conifer-dominated systems. However, conifer-dominated systems with low productivity also have low CWD mass. Stream size also influences CWD mass in lotic ecosystems, while successional stage dramatically influences CWD mass in boat aquatic and terrestrial settings. 5. CWD performs many functions in ecosystems, serving as auto-trophic and heterotrophic habitat and strongly influencing geomorphic processes, especially in streams. It is also a major component of nutrient cycles in many ecosystems. We have reviewed these many functions and conclude that CWD is an important functional component of stream and forest ecosystems. 6. Humans have greatly affected the amount of CWD found in temperate ecosystems by removing CWD and by changing the rate of input and the rate and pattern of loss. In some cases, human influences have been so pervasive that natural conditions are difficult to define. Management practices concerning CWD often have not been based on the numerous beneficial roles this material plays in ecosystems. Better scientific understanding of these functions and the natural factors influencing CWD dynamics should lead to more enlightened management practices. Uncited reference. {\textcopyright} 2004 Elsevier Inc. All rights reserved.},
annote = {This study reviews Coarse Woody Debris' (CWD) addition and removal in an ecosystem. This study also reviews the biomass in streams and forests and many of the functions that Coarse Woody Debris serves. There are many functions that control/ affect the rate at which CWD decomposes in an ecosystem. These factors can include temperature, moisture, internal gas composition, substrate quality the size of the debris, and the types of organisms involved in the ecosystem. Humans have greatly affected the amount of CWD found in these temperate ecosystems by changing the inputs and losses in an ecosystem. And more scientific understanding of these factors are needed to make accurate predictions and that will lead to better management practices. We can use this in our study to describe the many factors that must be considered when modeling fuel loads.},
author = {Harmon, M.E. and Franklin, J.F. and Swanson, F.J. and Sollins, P. and Gregory, S.V. and LATTIN, J.D. and ANDERSON, N.H. and CLINE, S.P. and AUMEN, N.G. and SEDELL, J.R. and LIENKAEMPER, G.W. and CROMACK, K. and CUMMINS, K.W.},
booktitle = {Advances in Ecological Research},
chapter = {1 - 7},
doi = {10.1016/S0065-2504(03)34002-4},
editor = {{David Bohan}, Alex Dumbrell},
file = {:D$\backslash$:/MendeleyDocs/HARMON et al. - 2004 - Ecology of Coarse Woody Debris in Temperate Ecosystems.pdf:pdf},
isbn = {0120139340},
issn = {00652504},
number = {03},
pages = {59--234},
pmid = {3866},
publisher = {Elsevier},
title = {{Ecology of Coarse Woody Debris in Temperate Ecosystems}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0065250403340024 http://www.sciencedirect.com/science/article/pii/S0065250403340024},
volume = {34},
year = {2004}
}
@article{DeGroot2007,
abstract = {In support of Canada's National Forest Carbon Monitoring, Accounting and Reporting System, a project was initiated to develop and test procedures for estimating direct carbon emissions from fires. The CanadianWildland Fire Information System (CWFIS) provides the infrastructure for these procedures. Area burned and daily fire spread estimates are derived from satellite products. Spatially and temporally explicit indices of burning conditions for each fire are calculated by CWFIS using fire weather data. The Carbon Budget Model of the Canadian Forest Sector (CBM- CFS3) provides detailed forest type and leading species information, as well as pre-fire fuel load data. The Boreal Fire Effects Model calculates fuel consumption for different live biomass and dead organic matter pools in each burned cell according to fuel type, fuel load, burning conditions, and resulting fire behaviour. Carbon emissions are calculated from fuel consumption. CWFIS summarises the data in the form of disturbance matrices and provides spatially explicit estimates of area burned for national reporting. CBM-CFS3 integrates, at the national scale, these fire data with data on forest management and other disturbances. The methodology for estimating fire emissions was tested using a large-fire pilot study.A framework to implement the procedures at the national scale is described.},
annote = {This study states that Canadian wildland fire activity has increased over the last 40 years. Much of the gas released in these fires is carbon dioxide. These authors initiated a project to estimate carbon emissions from fires. Satellites are used to estimate the spread of fire, explicit indices of burning conditions for each fire are calculated using fire weather data. At the national level, a framework to implement procedures on the project are described. The study concludes that the accurate estimation of carbon emissions require pre - fuel data and fire weather data for the whole burn period. The biggest challenge to this project is to acquire and annually update the necessary databases, at the correct times. We can use this in our study to estimate different fuel types and carbon emissions.},
author = {de Groot, William J. and Landry, Robert and Kurz, Werner A. and Anderson, Kerry R. and Englefield, Peter and Fraser, Robert H. and Hall, Ronald J. and Banfield, Ed and Raymond, Donald A. and Decker, Vincent and Lynham, Tim J. and Pritchard, Janet M.},
doi = {10.1071/WF06150},
file = {:D$\backslash$:/MendeleyDocs/de Groot et al. - 2007 - Estimating direct carbon emissions from Canadian wildland fires.pdf:pdf},
isbn = {1049-8001},
issn = {1049-8001},
journal = {International Journal of Wildland Fire},
keywords = {Fire behaviour,Fuel consumption,Remote sensing.},
number = {5},
pages = {593},
title = {{Estimating direct carbon emissions from Canadian wildland fires}},
url = {http://www.publish.csiro.au/?paper=WF06150 http://www.publish.csiro.au/wf/pdf/WF06150},
volume = {16},
year = {2007}
}
@article{Riccardi2007a,
abstract = {Wildland fuelbed characteristics are temporally and spatially complex and can vary widely across regions. To capture this variability, we designed the Fuel Characteristic Classification System (FCCS), a national system to create fuelbeds and classify those fuelbeds for their capacity to support fire and consume fuels. This paper describes the structure of the fuelbeds internal to FCCS. Fuelbeds are considered relatively homogeneous units on the landscape, representing distinct combustion environments that determine potential fire behaviour and effects. The FCCS fuelbeds are organized into six strata: canopy, shrubs, nonwoody fuels, woody fuels, litter-lichen-moss, and ground fuels. Fuelbeds are described by several qualitative and quantitative physical and biological variables with emphasis on characteristics useful for fuels management and fire behaviour planning. The FCCS includes 216 fuelbeds that represent the major vegetation types of the United States. The FCCS fuelbeds can be used as presented or modified to create customized fuelbeds with general or site-specific data to address fire science management or research questions. This system allows resource managers to evaluate wildland fuels operations and management activities, fire hazard, and ecological and air quality impacts at small and large spatial scales. The FCCS fuelbeds represent the United States, although the system has the potential for building fuelbeds for international application.},
annote = {This article describes the Fuel Character Classification System (FCCS), more specifically, it focuses on the fuel bed portion of the FCCS. The paper says that fuel beds are homogeneous units of the landscape. The FCCS fuel beds are organized into six strata; canopy, shrubs, ground fuels, litter-lichen-moss, non-woody fuels, and woody fuels. The FCCS includes 216 fuel beds that represent major vegetation types for ecosystems across the United States. The authors claim that this system allows resource managers to evaluate wildland fuels management activities, fire hazard, and ecological impacts at different scales. The FCCS fuel beds represent the United States, but the authors claim it has the potential for constructing fuel beds around the world. We can use this article in our study to determine fuel beds in the areas that we are studying.},
author = {Riccardi, Cynthia L and Ottmar, Roger D and Sandberg, David V and Andreu, Anne and Elman, Ella and Kopper, Karen and Long, Jennifer},
doi = {10.1139/X07-143},
file = {:D$\backslash$:/MendeleyDocs/Riccardi et al. - 2007 - The fuelbed a key element of the Fuel Characteristic Classification SystemThis article is one of a selection of.pdf:pdf},
isbn = {0045-5067},
issn = {0045-5067},
journal = {Canadian Journal of Forest Research},
keywords = {Fuel Characteristic Classification System},
month = {dec},
number = {12},
pages = {2394--2412},
title = {{The fuelbed: a key element of the Fuel Characteristic Classification System}},
url = {http://www.nrcresearchpress.com/doi/abs/10.1139/X07-143 http://www.nrcresearchpress.com/doi/abs/10.1139/X07-143{\#}.WRnla011qUl},
volume = {37},
year = {2007}
}
@techreport{Keane2012,
abstract = {We investigated the spatial variability of a number of wildland fuel characteristics for the major fuel components found in six common northern Rocky Mountain ecosystems. Surface fuel characteristics of loading, particle density, bulk density, and mineral content were measured for eight fuel components - four downed dead woody fuel size classes (1, 10, 100, 1000 hr), duff, litter, shrub, and herb - on nested plots located within sampling grids to describe their variability across spatial scales. We also sampled canopy bulk density, biomass, and cover for each plot in the grid. The spatial distribution and variability of surface and canopy fuel characteristics are described using the variance, spatial autocorrelation, semi-variograms, and Moran's I. We found that all fuels had high variability in loading (two to three times the mean), and this variability increased with the size of fuel particle. We also found that fuel components varied at different scales, with fine fuels varying at scales of 1 to 5 m, coarse fuels at 10 to 150 m, and canopy fuels at 100 to 500 m. Findings and data from this study can be used to sample, describe, and map fuel characteristics, such as loading, at the appropriate spatial scales to accommodate the next generation of fire behavior prediction models},
address = {Fort Collins, CO},
annote = {This is a study of spatial variability on several fuel characteristics for major components in several Rocky Mountain ecosystems. Loading, particle density, bulk density, and mineral content were measured for eight fuel components across a sample grid to describe the special scale. The eight components are four downed dead woody fuel size classes (1, 10, 100, 1000 hr.), duff, litter, shrub, and herb. They also sample the canopy over these spaces. The authors claim that the wildland fuel is the one factor that can be directly manipulated to achieve management goals, but it is difficult to measure. The researchers describe the special distribution using spatial autocorrection, semi-variograms, and Moran's I. They concluded that most of the fuel components have a variability of greater than 100 percent of the mean. And the variability increases with the increasing size of the fuel particles. The components also vary at different scales with smaller fuel particles varying over 1 m scales, large logs varying over 100 m, and canopy fuel attributes varying over 400 m. the data used in this study can be used to show variability in many different ecosystems and we can use it to show variability in our fuel loads.},
author = {Keane, Robert E and Gray, Kathy and Bacciu, Valentina},
file = {:D$\backslash$:/MendeleyDocs/Keane, Gray, Bacciu - 2012 - Spatial Variability of Wildland Fuel Characteristics in Northern Rocky Mountain Ecosystems.pdf:pdf},
institution = {United States Department of Agriculture},
isbn = {Res. Pap. RMRS-RP-98},
issn = {05025001},
keywords = {Biomass,Bulk density,Landscape ecology,Loading,Mineral content,Partical density,Spatial scale,Wildland fires,Woody debris},
number = {October},
pages = {56},
title = {{Spatial Variability of Wildland Fuel Characteristics in Northern Rocky Mountain Ecosystems}},
url = {https://www.researchgate.net/profile/Valentina{\_}Bacciu/publication/271586829{\_}Spatial{\_}variability{\_}of{\_}wildland{\_}fuel{\_}characteristics{\_}in{\_}northern{\_}Rocky{\_}Mountain{\_}ecosystems/links/54cd0a8f0cf298d6565c9063.pdf},
year = {2012}
}
@article{Arroyo2008,
abstract = {Understanding fire is essential to improving forest management strategies. More specifically, an accurate knowledge of the spatial distribution of fuels is critical when analyzing, modelling and predicting fire behaviour. First, we review the main concepts and terminology associated with forest fuels and a number of fuel type classifications. Second, we summarize the main techniques employed to map fuel types starting with the most traditional approaches, such as field work, aerial photo interpretation or ecological modelling. We pay special attention to more contemporary techniques, which involve the use of remote sensing systems. In general, remote sensing systems are low-priced, can be regularly updated and are less time-consuming than traditional methods, but they are still facing important limitations. Recent work has shown that the integration of different sources of information and methods in a complementary way helps to overcome most of these limitations. Further research is encouraged to develop novel and enhanced remote sensing techniques. ?? 2008 Elsevier B.V. All rights reserved.},
annote = {This study reviews the methods of creating fire models. The traditional fuel mapping techniques like, field work, aerial photography and ecological modeling suffer some limitations. The main issue was that these methods take a great amount of field work which is very time consuming and can be very expensive. The method of remote sensing offers to use a large variety of systems ad this can give a more accurate impression of the forest. They conclude with stating that future research should focus on the integration of mapping techniques and sensor data. The approach, that they state, has been used to map vegetation and they say that this could be used as a method to map fuel types. This review can be used to categorize and measure fuel types in our study.},
author = {Arroyo, Lara A and Pascual, Cristina and Manzanera, Jos{\'{e}} A},
doi = {10.1016/j.foreco.2008.06.048},
file = {:D$\backslash$:/MendeleyDocs/Arroyo, Pascual, Manzanera - 2008 - Fire models and methods to map fuel types The role of remote sensing.pdf:pdf},
isbn = {0378-1127},
issn = {03781127},
journal = {Forest Ecology and Management},
keywords = {Forest fuels,Fuel management,Fuel mapping,Fuel type,Remote sensing},
month = {sep},
number = {6},
pages = {1239--1252},
title = {{Fire models and methods to map fuel types: The role of remote sensing}},
url = {https://www.srs.fs.usda.gov/pubs/gtr/gtr{\%}7B{\_}{\%}7Dsrs080/gtr{\%}7B{\_}{\%}7Dsrs080.pdf http://linkinghub.elsevier.com/retrieve/pii/S0378112708005410},
volume = {256},
year = {2008}
}
